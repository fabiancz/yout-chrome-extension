{
   "author": "Lukas Vana",
   "background": {
      "scripts": [ "background.js" ]
   },
   "description": "Shortcut for downlad video by yout.com (click on download icon next to Youtube video URL)",
   "icons": {
      "128": "images/icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Yout",
   "page_action": {
      "default_icon": {
         "19": "images/icon19.png",
         "38": "images/icon38.png"
      },
      "default_title": "Download"
   },
   "permissions": [ "tabs", "https://www.youtube.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
